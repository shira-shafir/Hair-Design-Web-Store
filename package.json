{
  "name": "finalprojectshira",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "concurrently": "^6.0.0"
  },
  "scripts": {
    "start": "./node_modules/.bin/concurrently \"npm run start-client\" \"npm run start-server\"",
    "build": "npm install && npm run build-client && npm run build-server",
    "start-client": "npm start --prefix frontend",
    "start-server": "npm start --prefix backend",
    "build-client": "npm build --prefix frontend",
    "build-server": "npm build --prefix backend"
  }
}
