{
  "name": "finalprojectshira",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "concurrently": "^6.0.0"
  },
  "scripts": {
    "start": "./node_modules/.bin/concurrently \"npm run start-client\" \"npm run start-server\"",
    "build": "npm install && npm run build-client && npm run build-server",
    "start-client": "cd frontend && npm start",
    "start-server": "cd backend && npm start",
    "build-client": "cd frontend && npm install && npm run build",
    "build-server": "cd backend && npm install"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "react-player": "^2.9.0",
    "react-youtube": "^7.13.1"
  }
}
